{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --host 0.0.0.0",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "npm run smui-theme-light",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@smui/button": "^6.0.0",
    "@smui/circular-progress": "^6.0.0",
    "@smui/layout-grid": "^6.0.0",
    "@smui/paper": "^6.0.0",
    "@smui/textfield": "^6.0.0",
    "@smui/top-app-bar": "^6.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "postcss": "^8.4.16",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "smui-theme": "^6.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bx24-api": "^1.4.1",
    "sirv-cli": "^2.0.0",
    "svelte-material-ui": "^6.0.0",
    "svelte-routing": "^1.6.0"
  }
}
